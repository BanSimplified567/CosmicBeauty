<IfModule mod_dir.c>
    DirectoryIndex index.php
</IfModule>

<IfModule mod_rewrite.c>
    RewriteEngine On

    # Redirect all requests to index.php if file/folder does not exist
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule ^ index.php [L]
</IfModule>
